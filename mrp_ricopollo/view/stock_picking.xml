<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

        <record id="init_view_picking_form" model="ir.ui.view">
            <field name="name">stock.picking.form.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
	            <xpath expr="//button[@name='%(stock.act_stock_return_picking)d']" position="after">
                    <button name="make_cost_price_journal_entry" string="Make Cost Price Entries" type="object"/>
                </xpath>
            </field>
        </record>
        
	</data>
</openerp>
